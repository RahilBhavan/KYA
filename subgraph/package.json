{
  "name": "kya-protocol-subgraph",
  "version": "1.0.0",
  "description": "The Graph subgraph for KYA Protocol",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ <subgraph-name>",
    "deploy:local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 <subgraph-name>",
    "create:local": "graph create --node http://localhost:8020/ <subgraph-name>",
    "remove:local": "graph remove --node http://localhost:8020/ <subgraph-name>"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.60.0",
    "@graphprotocol/graph-ts": "^0.32.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}
